                <LayoutItem Id="Department.Office" ViewItem="Department.Office" Index="4" RelativeSize="24" IsNewNode="True" />
              </LayoutGroup>
              <LayoutGroup Id="Contact_col2" RelativeSize="50.149551345962116">
                <LayoutItem Id="Anniversary" RelativeSize="24" />
